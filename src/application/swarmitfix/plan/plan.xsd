  <xs:element name="bCind" type="xs:integer"/>
  <xs:element name="cx" type="xs:decimal"/>
  <xs:element name="cy" type="xs:decimal"/>
  <xs:element name="cz" type="xs:decimal"/>
  <xs:element name="theta" type="xs:double"/>
  <xs:element name="pkmTheta" type="xs:decimal"/>
  <xs:element name="ABeg" type="xs:decimal"/>
  <xs:element name="AHoldMax" type="xs:decimal"/>
  <xs:element name="AHoldMin" type="xs:decimal"/>
  <xs:element name="dPkmTheta" type="xs:decimal"/>
  <xs:element name="dTheta" type="xs:decimal"/>
  <xs:element name="pin" type="xs:integer"/>
  <xs:element name="k1" type="xs:decimal"/>
  <xs:element name="k2" type="xs:decimal"/>
  <xs:element name="line">
    <xs:complexType>
      <xs:sequence>
