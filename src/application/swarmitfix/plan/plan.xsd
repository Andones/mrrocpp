  <xs:element name="bcy" type="xs:decimal"/>
  <xs:element name="dmin" type="xs:decimal"/>
  <xs:element name="dmax" type="xs:decimal"/>
  <xs:element name="dcen" type="xs:decimal"/>
  <xs:element name="dh" type="xs:decimal"/>
  <xs:element name="eps" type="xs:decimal"/>
  <xs:element name="alpha0" type="xs:decimal"/>
  <xs:element name="l1" type="xs:decimal"/>
  <xs:element name="l2" type="xs:decimal"/>
  <xs:element name="l3" type="xs:decimal"/>
  <xs:element name="psi1" type="xs:decimal"/>
  <xs:element name="psi2" type="xs:decimal"/>
  <xs:element name="psi3" type="xs:decimal"/>
  <xs:element name="beta7" type="xs:decimal"/>
  <xs:element name="pkmbaseToFixedPkm" type="xs:decimal"/>
  <xs:element name="hlowered" type="xs:decimal"/>
  <xs:element name="Dz" type="xs:integer"/>
  <xs:element name="maxPkmNeutralDist" type="xs:integer"/>
  <xs:element name="PX" type="xs:decimal"/>
  <xs:element name="PZ" type="xs:decimal"/>
  <xs:element name="COSGA" type="xs:decimal"/>
  <xs:element name="SINGA" type="xs:decimal"/>
  <xs:element name="agent" type="xs:string"/>
  <xs:element name="TBeg" type="xs:string"/>
  <xs:element name="TEnd" type="xs:string"/>
  <xs:element name="sP" type="xs:integer"/>
  <xs:element name="eP" type="xs:integer"/>
  <xs:element name="sPos" type="xs:string"/>
