        <xs:element ref="PKM_Max_Accel"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="lineList">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" name="item">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="sP"/>
              <xs:element ref="eP"/>
              <xs:element ref="sPos"/>
              <xs:element ref="ePos"/>
