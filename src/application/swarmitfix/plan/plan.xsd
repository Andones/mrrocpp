              <xs:element ref="b"/>
              <xs:element ref="c"/>
              <xs:element ref="norm"/>
              <xs:element ref="norm2"/>
              <xs:element ref="alfa"/>
              <xs:element ref="beta"/>
              <xs:element ref="gamma"/>
              <xs:element ref="len"/>
              <xs:element ref="index"/>              
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
