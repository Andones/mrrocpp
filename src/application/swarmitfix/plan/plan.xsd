<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="plan">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="svar">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="item">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element ref="sHind"/>
                    <xs:element ref="eHind"/>
                    <xs:element ref="numH"/>
                    <xs:element ref="sBind"/>
                    <xs:element ref="eBind"/>
                    <xs:element ref="numB"/>
                    <xs:element ref="sPind"/>
                    <xs:element ref="ePind"/>
                    <xs:element ref="numP"/>
                    <xs:element ref="segInd"/>
                    <xs:element ref="agent"/>
                    <xs:element ref="TBeg"/>
                    <xs:element ref="TEnd"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element ref="sNum"/>
        <xs:element name="head">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="item">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element ref="ind"/>
                    <xs:element ref="cx"/>
                    <xs:element ref="cy"/>
                    <xs:element ref="cz"/>
