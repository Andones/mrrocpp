                    <xs:element ref="bcx"/>
                    <xs:element ref="bcy"/>
                    <xs:element ref="dmin"/>
                    <xs:element ref="dmax"/>
                    <xs:element ref="dcen"/>
                    <xs:element ref="dh"/>
                    <xs:element ref="eps"/>
                    <xs:element ref="agent"/>
                    <xs:element ref="TBeg"/>
                    <xs:element ref="TEnd"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element ref="hNum"/>
        <xs:element name="mbase">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="item">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element ref="ind"/>
                    <xs:element ref="bCind"/>
