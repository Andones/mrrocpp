                    <xs:element ref="beta"/>
                    <xs:element ref="gamma"/>
                    <xs:element ref="line"/>
                    <xs:element ref="lineseg"/>
                    <xs:element name="actions" type="xs:string"/>
                    <xs:element ref="numActions"/>
                    <xs:element ref="k1"/>
                    <xs:element ref="k2"/>
